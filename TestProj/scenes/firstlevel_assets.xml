<root>
	<!-- Shaders -->
	<asset type="ShaderAsset" id="DiffuseShader">
		<vert>Assets/Shaders/Diffuse.vert</vert>
		<frag>Assets/Shaders/Diffuse.frag</frag>
	</asset>

	<asset type="ShaderAsset" id="FlatShader">
		<vert>Assets/Shaders/Flat.vert</vert>
		<frag>Assets/Shaders/Flat.frag</frag>
	</asset>

	<!-- Textures -->
	<asset type="TextureAsset" id="LizardManDiffuse">
		<path>Assets/Textures/Lizardman_col.png</path>
		<format>rgba</format>
		<type>2d</type>
	</asset>

	<!-- Meshes -->
	<asset type="MeshAsset" id="LizardMan">
		<path>Assets/Models/lizardman.mesh</path>
	</asset>
	
	
	<!-- Materials -->
	<asset type="MaterialAsset" id="lizardman_material">
		<path>Assets/Materials/lizardman.material</path>
	</asset>
</root>